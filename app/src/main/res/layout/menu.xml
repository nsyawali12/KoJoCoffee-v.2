<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="47dp"
        android:layout_marginTop="58dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/espresso" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="67dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/cappucinno" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignStart="@+id/imageButton"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="208dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/latte" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="67dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/v60" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignStart="@+id/imageButton"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="35dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/kalita" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="98dp"
        android:layout_height="119dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="32dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/caramel" />
</RelativeLayout>
